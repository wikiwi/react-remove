{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
